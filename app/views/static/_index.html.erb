    <%= link_to "Sellbrite", "https://app.sellbrite.com/orders", class: "btn btn-primary" %>
    <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">Order Ref</th>
          <th scope="col">Shipping Method</th>
          <th scope="col">Sales Channel</th>
          <th scope="col">Qty</th>
